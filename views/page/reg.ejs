<%- include ('../partials/navigation.ejs') %>

<div class="container">
  <main class="wrapper">
    <form id="formSignup">
      <h2>Regsiter here!</h2>

      <input
        type="email"
        id="email"
        name="email"
        placeholder="Enter your e-mail"
      />
      <input
        type="password"
        id="password"
        name="password"
        placeholder="Enter a password"
      />
      <input
        type="password"
        id="repeatPassword"
        name="repeatPassword"
        placeholder="Verify password"
      />
      <button>Register</button>
    </form>
  </main>
</div>

